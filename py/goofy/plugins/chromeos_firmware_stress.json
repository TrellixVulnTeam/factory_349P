{
  "backends": [
    {
      "name": "time_sanitizer",
      "args": {
        "base_time_files": ["/usr/local/etc/lsb-factory",
                            "/etc/lsb-release"]
      }
    },
    {
      "name": "disk_monitor",
      "args": {
        "period_secs": 120,
        "stateful_usage_threshold": 90
      }
    },
    {
      "name": "battery_monitor",
      "args": {
        "period_secs": 120,
        "critical_low_battery_pct": 5,
        "warning_low_battery_pct": 10
      }
    },
    {
      "name": "charge_manager",
      "args": {
        "period_secs": 120,
        "min_charge_pct": 88,
        "max_charge_pct": 89
      }
    },
    {
      "name": "cpu_usage_monitor",
      "args": {
        "period_secs": 120
      }
    },
    {
      "name": "cpu_freq_manager"
    },
    {
      "name": "connection_manager",
      "args": {
        "wlans": [
          {
            "ssid": "crosfactory20",
            "security": "psk",
            "passphrase": "crosfactory"
          },
          {
            "ssid": "crosfactory21",
            "security": "psk",
            "passphrase": "crosfactory"
          }
        ],
        "shuffle_wlans": true,
        "scan_wifi_periods_secs": 10
      }
    },
    {
      "name": "thermal_monitor",
      "args": {
        "period_secs": 120,
        "delta_threshold": 1.1
      }
    },
    {
      "name": "system_log_manager",
      "args": {
        "scan_log_period_secs": 120,
        "sync_log_period_secs": 600,
        "sync_log_paths": [
          "/var/factory/log/",
          "/var/log/messages",
          "/var/log/Xorg.0.log",
          "/var/log/bios_info.txt",
          "/var/log/ec_info.txt"],
        "clear_log_paths": [
          "/var/log/connectivity.log",
          "/var/log/connectivity.bak",
          "/var/log/net.log",
          "/var/log/net.*.log",
          "/var/log/messages*",
          "/var/spool/crash/shill.*"],
        "clear_log_excluded_paths": [
          "/var/log/messages", "/var/log/messages.[12]"],
        "enable_foreground_sync": true
      }
    },
    {
      "name": "core_dump_manager",
      "args": {
        "period_secs": 120
      }
    }
  ]
}
