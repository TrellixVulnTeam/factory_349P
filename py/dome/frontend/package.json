{
  "name": "dome",
  "version": "1.2.1",
  "description": "the factory server front end",
  "dependencies": {
    "deepcopy": "^0.6.3",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "material-ui": "^0.15.4",
    "material-ui-chip-input": "^0.3.2",
    "react": "~15.3.1",
    "react-dom": "~15.3.1",
    "react-measure": "^0.5.0",
    "react-redux": "^4.4.5",
    "react-sortable-hoc": "0.0.7",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2",
    "redux-immutable": "^3.0.6",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "~6.9.0",
    "babel-preset-react": "~6.5.0",
    "babel-preset-stage-2": "^6.13.0",
    "babelify": "~7.3.0",
    "browserify": "~13.0.1",
    "eslint": "^3.0.1",
    "eslint-config-defaults": "^9.0.0",
    "eslint-plugin-react": "^5.2.2"
  },
  "scripts": {
    "build": "browserify --debug --transform [ babelify ] --outfile bundle.js --entry index.js",
    "lint": "eslint index.js components constants actions reducers"
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-2"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true,
        "jsx": true
      },
      "sourceType": "module"
    },
    "plugins": [
      "react"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "rules": {
      "indent": ["error", 2, {"SwitchCase": 1}],
      "linebreak-style": ["error", "unix"],
      "quotes": ["error", "single"],
      "semi": ["error", "always"],
      "no-console": ["error", {"allow": ["error", "warn"]}],
      "no-unused-vars": ["error", {"varsIgnorePattern": "unused|ignored"}]
    }
  },
  "author": "Mao Huang <littlecvr@google.com>"
}
