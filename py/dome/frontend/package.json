{
  "name": "dome",
  "version": "1.3.0",
  "description": "the factory server front end",
  "dependencies": {
    "axios": "~0.18.0",
    "date-and-time": "~0.6.2",
    "immutable": "4.0.0-rc.9",
    "material-ui": "~0.20.1",
    "material-ui-chip-input": "~0.19.0",
    "prop-types": "~15.6.1",
    "react": "~16.4.0",
    "react-dom": "~16.4.0",
    "react-measure": "~1.4.7",
    "react-redux": "~5.0.7",
    "react-sortable-hoc": "0.7.4",
    "redux": "~4.0.0",
    "redux-form": "~7.3.0",
    "redux-immutable": "~4.0.0",
    "redux-logger": "~3.0.6",
    "redux-thunk": "~2.2.0",
    "uuid": "~3.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.49",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.49",
    "@babel/preset-env": "7.0.0-beta.49",
    "@babel/preset-react": "7.0.0-beta.49",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "~8.2.3",
    "babel-plugin-module-resolver": "~3.1.1",
    "babelify": "~9.0.0",
    "browserify": "~16.2.2",
    "eslint": "~4.19.1",
    "eslint-config-google": "~0.9.1",
    "eslint-plugin-babel": "~5.1.0",
    "eslint-plugin-react": "~7.8.2"
  },
  "scripts": {
    "build": "mkdir -p build && cp src/static/* build && browserify --debug --outfile build/bundle.js --entry src/index.js",
    "lint": "eslint src"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-react",
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": ">= 1%, not ie <= 11"
          },
          "useBuiltIns": "usage"
        }
      ]
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "alias": {
            "@app": "./src",
            "@common": "./src/common"
          }
        }
      ],
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "author": "Mao Huang <littlecvr@google.com>"
}
