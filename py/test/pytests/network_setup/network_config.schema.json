          },
          "required": false,
          "type": "array"
        },
        "prefixlen": {
          "required": false,
          "type": "number"
        },
        "mtu": {
          "required": false,
          "type": "number"
        },
        "gateway": {
          "required": false,
          "type": "string",
          "pattern": "^(?:(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\\.){3}(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)$"
        },
        "address": {
          "pattern": "^(?:(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\\.){3}(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)$",
